{
    "version": 2,
    "controller": {
      "id": "lienode01",
      "options": {
         "title": "liestudio-controller"
      }
    },
    "workers": [
        {
            "type": "router",
            "realms": [
                {
                    "name": "liestudio",
                    "roles": [
                        {
                            "name": "public",
                            "permissions": [
                                {
                                    "uri": "",
                                    "match": "prefix",
                                    "allow": {
                                        "call": true,
                                        "register": true,
                                        "publish": true,
                                        "subscribe": true
                                    },
                                    "disclose": {
                                        "caller": false,
                                        "publisher": false
                                    },
                                    "cache": true
                                }
                            ]
                        }
                    ]
                }
            ],
            "transports": [
                {
                    "type": "web",
                    "endpoint": {
                        "type": "tcp",
                        "port": 8083,
                        "tls": {
                            "certificate": "server.crt",
                            "key": "server.key",
                            "dhparam": "dhparam",
                            "ca_certificates": [
                                "intermediate.cert.pem",
                                "ca.cert.pem"
                            ]
                        }
                    },
                    "paths": {
                        "/": {
                            "type": "static",
                            "directory": "../../app/dist"
                        },
                        "help": {
                            "type": "static",
                            "directory": "../../docs/html"
                        },
                        "ws": {
                            "type": "websocket",
                            "auth": {
                                "anonymous": {
                                    "type": "static",
                                    "role": "public"
                                }
                            }
                        }
                    }
                }
            ]
        },
        {
            "type": "container",
            "options": {
                "pythonpath": [
                    ".."
                ]
            },
            "components": [
              {
                "type": "wamplet",
                "package": "lie_user",
                "realm": "liestudio",
                "entrypoint": "wamp_services",
                "transport": {
                  "type": "websocket",
                  "endpoint": {
                     "type": "tcp",
                     "host": "localhost",
                     "port": 8083,
                     "tls": {
                         "hostname": "localhost",
                         "certificate": "client.crt",
                         "key": "client.key",
                         "ca_certificates": [
                             "intermediate.cert.pem",
                             "ca.cert.pem"
                         ]
                     }
                  },
                  "url": "wss://localhost:8083/ws"
                }
              },
              {
                "type": "wamplet",
                "package": "lie_config",
                "realm": "liestudio",
                "entrypoint": "wamp_services",
