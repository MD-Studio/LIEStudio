{
    "crossbar": {
        "start": {
            "cbdir": "{system.app_path}/data/crossbar/", 
            "config": "{crossbar.start.cbdir}config_notls.json", 
            "logdir": "{system.app_path}/data/logs", 
            "loglevel": "info", 
            "logtofile": true
        }
    }, 
    "lie_config": null, 
    "lie_db": {
        "create_db": true, 
        "dblog": "{system.app_path}/data/logs/mongodb.log", 
        "dbname": "liestudio", 
        "dbpath": "{system.app_path}/data/liedb", 
        "host": "mongo", 
        "port": 27017, 
        "terminate_mongod_on_exit": false
    }, 
    "lie_docking": {
        "plants": {
            "bindingsite_center": [
                0.0, 
                0.0, 
                0.0
            ], 
            "bindingsite_radius": 12, 
            "chemplp_charged_hb_weight": 2.0, 
            "chemplp_charged_metal_weight": 2.0, 
            "chemplp_clash_include_14": 1, 
            "chemplp_clash_include_HH": 0, 
            "chemplp_hbond_cho_weight": -3.0, 
            "chemplp_hbond_weight": -3.0, 
            "chemplp_intercept_weight": -20.0, 
            "chemplp_lipo_weight": 0.0, 
            "chemplp_metal_weight": -6.0, 
            "chemplp_plp_burpolar_e": -0.1, 
            "chemplp_plp_hbond_e": -1.0, 
            "chemplp_plp_metal_e": -1.0, 
            "chemplp_plp_repulsive_weight": 1.0, 
            "chemplp_plp_steric_e": -0.4, 
            "chemplp_plp_weight": 1.0, 
            "chemplp_tors_weight": 2.0, 
            "chemplp_weak_cho": 1, 
            "cluster_rmsd": 1.0, 
            "cluster_structures": 50, 
            "enable_sulphur_acceptors": 0, 
            "exec_path": "{system.app_path}/bin/plants_{system.platform}", 
            "flip_amide_bonds": 1, 
            "flip_planar_n": 1, 
            "flip_ring_corners": 0, 
            "force_flipped_bonds_planarity": 0, 
            "force_planar_bond_rotation": 1, 
            "ligand_file": "ligand.mol2", 
            "ligand_intra_score": "clash2", 
            "merge_multi_conf_output": 0, 
            "output_dir": ".", 
            "outside_binding_site_penalty": 50.0, 
            "plp_burpolar_e": -0.05, 
            "plp_hbond_e": -2.0, 
            "plp_metal_e": -4.0, 
            "plp_repulsive_weight": 0.5, 
            "plp_steric_e": -0.4, 
            "plp_tors_weight": 1.0, 
            "protein_file": "protein.mol2", 
            "rescore_mode": "simplex", 
            "scoring_function": "chemplp", 
            "search_speed": "speed1", 
            "write_merged_ligand": 0, 
            "write_merged_protein": 0, 
            "write_merged_water": 0, 
            "write_multi_mol2": 0, 
            "write_per_atom_scores": 0, 
            "write_protein_bindingsite": 0, 
            "write_protein_conformations": 0, 
            "write_ranking_links": 0
        }
    }, 
    "lie_logger": {
        "global_log_level": "info", 
        "observers": {
            "ExportToMongodbObserver": {
                "activate": true, 
                "log_cache_size": 50
            }, 
            "PrintingObserver": {
                "activate": true, 
                "datefmt": "%m/%d/%Y %H:%M:%S", 
                "filter_predicate": {
                    "app_namespace": " > 5", 
                    "log_level": "{lie_logger.global_log_level}"
                }, 
                "format_event": "{asctime} - [{log_level.name:<5}: {log_namespace}] - {message}\n"
            }, 
            "RotateFileLogObserver": {
                "activate": true, 
                "datefmt": "%m/%d/%Y %H:%M:%S", 
                "format_event": "{asctime} - [{log_level.name:<5}: {log_namespace}] - {message}\n", 
                "logfile_path": "{system.app_path}/data/logs/lielog.log", 
                "rotation_time": 86400
            }
        }
    }, 
    "lie_user": {
        "admin_email": "m4.van.dijk@vu.nl", 
        "admin_password": "liepw@#", 
        "admin_username": "lieadmin", 
        "db_collection_name": "users", 
        "email_smtp_password": "<password>", 
        "email_smtp_port": 587, 
        "email_smtp_server": "smtp.gmail.com", 
        "email_smtp_username": "marcvdijk@gmail.com", 
        "email_smtp_usetls": null, 
        "has_method": "sha256", 
        "hash_iteration": 1000, 
        "key_derivation": "pbkdf2", 
        "random_password_length": 10, 
        "salt_length": 10
    }, 
    "system": {
        "app_config": "{system.app_path}/data/settings.json", 
        "app_path": "/app", 
        "bootstrap_order": [
            "lie_db", 
            "lie_logger", 
            "lie_user"
        ], 
        "is_dev_build": true, 
        "is_docker_build": true, 
        "platform": "linux2", 
        "shutdown_order": [
            "lie_user", 
            "lie_logger", 
            "lie_db"
        ]
    }
}