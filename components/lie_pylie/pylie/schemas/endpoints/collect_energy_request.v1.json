{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "id": "http://liestudio/schemas/collect_energies_request.json",
    "title": "Calculated lie average",
    "description": "Import GROMACS MD trajectory energy files into a LIEMDFrame",
    "type": "object",
    "properties": {
	"bound_trajectory": {
	    "type": "array",
	    "description": "protein ligand simulations"
	},
	"unbound_trajectory": {
	    "type": "array",
	    "description": "Ligand in solvent simulation"
	},
	"filetype":{
	    "type": "string",
	    "default": "gromacs"
	},
	"lie_vdw_header": {
	    "type": "string",
	    "default": "vdwLIE",
	    "description": "van der Waals term"
	},
	"lie_ele_header": {
	    "type": "string",
	    "default": "EleLIE",
	    "description": "Electronic term"
	},
	"case": {
	    "type": "number",
	    "default": 1
	},
	"workdir": {
	    "type": "string",
	    "default": "."
	}
    },
    "required": ["bound_trajectories", "unbound_trajectories"]
}
