ALA:
    atoms:
        - name: N
          vdw-type: N
          mass-type: N
          charge: null
          charge-type: backbone-N
          charge-group: 0
        - name: H
          vdw-type: H
          mass-type: H
          charge: null
          charge-type: backbone-H
          charge-group: 0
        - name: CA
          vdw-type: CH1
          mass-type: CH1
          charge: null
          charge-type: ala_val_ile_leu-CH
          charge-group: 1
        - name: CB
          vdw-type: CH3
          mass-type: CH3
          charge: null
          charge-type: ala_val_ile_leu-CH3
          charge-group: 1
        - name: C
          vdw-type: C
          mass-type: C
          charge: null
          charge-type: backbone-C
          charge-group: 2
        - name: O
          vdw-type: O
          mass-type: O
          charge: null
          charge-type: backbone-O
          charge-group: 2
    
    bonds:
        - indices: [ N, H ]
          type: gb_2
        - indices: [ N, CA ]
          type: gb_21
        - indices: [ CA, CB ]
          type: gb_27
        - indices: [ CA, C ]
          type: gb_27
        - indices: [ C, O ]
          type: gb_5
        - indices: [ C, +N  ]
          type: gb_10                             
               
    angles:
        - indices: [ -C, N, H ]
          type: ga_32
        - indices: [ -C, N, CA ]
          type: ga_31          
        - indices: [ H, N, CA ]
          type: ga_18                      
        - indices: [ N, CA, CB ]
          type: ga_13               
        - indices: [ N, CA, C ]
          type: ga_13            
        - indices: [ CB, CA, C ]
          type: ga_13               
        - indices: [ CA, C, O ]
          type: ga_30                 
        - indices: [ CA, C, +N ]
          type: ga_19         
        - indices: [ O, C, +N ]
          type: ga_33    

    impropers:
        - indices: [ N, -C, CA, H ]
          type: gi_1
        - indices: [ CA, N, C, CB ]
          type: gi_2              
        - indices: [ C, CA, +N, O ]
          type: gi_1          
    
    dihedrals:
        - indices: [ -CA, -C, N, CA ]
          type: gd_14
        - indices: [ -C, N, CA, C ]
          type: gd_39
        - indices: [ N, CA, C, +N ]
          type: gd_40


        
        

         

    