{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "id": "http://mdstudio/schemas/propka_request.json",
    "title": "propka input",
    "description": "predicts the pKa values of ionizable groups in proteins",
    "type": "object",
    "properties": {
	"pdb": {
	    "type": "string",
	    "description": "Protein file"
	},
	"parameters": {
	    "type": "string",
	    "description": "Path to the parameters file",
	    "default": "propka.cfg"
	},
	"from_file": {
	    "type": "boolean",
	    "default": "false"
	},
	"workdir": {
	    "type": "string",
	    "default": "."
	}
    },
    "required": ["pdb"]
}
